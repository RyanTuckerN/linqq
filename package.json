{
  "name": "linqq",
  "version": "1.0.5",
  "description": "Why use arrays when you can use linqq? Full LINQ implementation for JavaScript & TypeScript. Powerful iterable utilities inspired by .NET's LINQ",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "test:perf": "jest --config jest.perf.config.js",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "linq",
    "dotnet",
    "arrays",
    "iterable",
    "enumerable",
    "list",
    "collection",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryantuckern/linqq.git"
  },
  "bugs": {
    "url": "https://github.com/ryantuckern/linqq/issues"
  },
  "homepage": "https://github.com/ryantuckern/linqq#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "author": "Ryan Tucker",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "terser": "^5.31.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typescript": "~5.7.2"
  }
}
